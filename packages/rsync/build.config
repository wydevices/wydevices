NAME=rsync

VERSION=3.1.1

URL="http://mirrors.ibiblio.org/rsync/src/rsync-$VERSION.tar.gz"

SRCFILE="rsync-$VERSION.tar.gz"

SRCDIR="rsync-$VERSION"

CONFIGUREFLAGS="--host=sh4-linux \
	--prefix=/wymedia/usr \
	--disable-locale \
	--disable-acl-support \
	--disable-xattr-support \
	--with-rsyncd-conf=/wymedia/usr/etc/rsyncd.conf"

BINARIES="rsync"

POST_COMPILATION() {
	sh4-linux-strip $SRCDIR/$BINARIES
}
