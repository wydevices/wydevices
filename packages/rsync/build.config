NAME=rsync

VERSION=3.0.7

URL="http://samba.anu.edu.au/ftp/rsync/src/rsync-3.0.7.tar.gz"

SRCFILE="rsync-3.0.7.tar.gz"

SRCDIR="rsync-3.0.7"

CONFIGUREFLAGS="--host=sh4-linux \
	--prefix=/wymedia/usr \
	--disable-locale \
	--disable-acl-support \
	--disable-xattr-support \
	--with-rsyncd-conf=/wymedia/usr/etc/rsyncd.conf"

BINARIES="rsync"

POST_COMPILATION() {
	sh4-linux-strip $SRCDIR/$BINARIES
}

