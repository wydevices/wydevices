NAME="fb2png"

VERSION="1.0"

DEPENDS="libpng"

SRCDIR="fb2png_src"

PRE_COMPILATION() {
	cd $PKGDIR/$SRCDIR
	sh4-linux-gcc -Wall fb2png.c -L$PKGDIR/depends/libpng/libpng-1.5.6/.libs -I$PKGDIR/depends/libpng/libpng-1.5.6 -lpng15 -lz -o fb2png
}

BINARIES="fb2png"

POST_COMPILATION() {
	sh4-linux-strip $SRCDIR/fb2png
}
