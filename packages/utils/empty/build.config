NAME=empty

VERSION=0.6.18b

URL="https://wydevices.googlecode.com/svn/trunk/packages/source/empty-$VERSION.tgz"

SRCFILE="$NAME-$VERSION.tgz"

SRCDIR="$NAME-$VERSION"

BINARIES="$NAME"

DESTFILE="wybox-$NAME-$VERSION.tar.gz"

POST_COMPILATION() {
	cd $SRCDIR
	rm empty
	sh4-linux-gcc -Os -Wall -lutil -o empty empty.c
	sh4-linux-strip $NAME
}
