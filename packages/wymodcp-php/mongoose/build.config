NAME="mongoose"

VERSION="3.0"

URL="http://mongoose.googlecode.com/files/mongoose-3.0.tgz"

SRCFILE="mongoose-3.0.tgz"

SRCDIR="mongoose"

PRE_COMPILATION() {

	cd $PKGDIR/$SRCDIR
	make COPT="-DNDEBUG -DNO_SSL" linux
}

BINARIES="mongoose"

POST_COMPILATION() {
	sh4-linux-strip $SRCDIR/$BINARIES
}

